SRC	=	main.c			\
		get_next_line.c		\
		my_strlen.c		\
		print_tab.c		\
		create_maillon.c	\
		get_maze.c		\

NAME	=	solver

CC	=	gcc

OBJ	=	$(SRC:.c=.o)

RM 	=	rm -f

CFLAGS	=	-W -Wextra -ansi

$(NAME)	:	$(OBJ)
		$(CC) -o $(NAME) $(OBJ)

all	:	$(NAME)

clean	:	
		$(RM) $(OBJ)

fclean	:	clean
		$(RM) $(NAME)

re	:	fclean all
